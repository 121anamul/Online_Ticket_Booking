<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <servlet>
        <servlet-name>Registration</servlet-name>
        <servlet-class>Registration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Buyonline</servlet-name>
        <servlet-class>Buyonline</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>forget</servlet-name>
        <servlet-class>forget</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChangePass</servlet-name>
        <servlet-class>ChangePass</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>emailcon</servlet-name>
        <servlet-class>emailcon</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ChangePass</servlet-name>
        <url-pattern>/ChangePass</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>emailcon</servlet-name>
        <url-pattern>/emailcon</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>forget</servlet-name>
        <url-pattern>/forget</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Registration</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Buyonline</servlet-name>
        <url-pattern>/online</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>Buy.jsp</welcome-file>
    </welcome-file-list>
    
</web-app>